
	 <ul class="pagination">
	  {{#if next}}
	  	<li class="waves-effect pagination-item older"><a href="{{page_url next}}">&larr;</a></li>
	  {{else}}
	    <li class="disabled pagination-item older"><a href="#!">&larr;</a></li>
	  {{/if}}
	  {{#if prev}}
	  	<li class="waves-effect pagination-item newer"><a href="{{page_url prev}}">&rarr;</a></li>
	  {{else}}
	  	<li class="disabled pagination-item newer"><a href="#!">&rarr;</a></li>
	  {{/if}}
	 </ul>
